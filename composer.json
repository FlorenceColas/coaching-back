{
    "name": "coaching-back",
    "description": "Coaching API",
    "type": "project",
    "homepage": "https://github.com/FlorenceColas/coaching-back",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "ext-pdo": "*",
        "firebase/php-jwt": "^5.0",
        "zendframework/zend-authentication": "^2.7",
        "zendframework/zend-component-installer": "^2.1.1",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-db": "^2.10",
        "zendframework/zend-diactoros": "^1.7.1 || ^2.0",
        "zendframework/zend-expressive": "^3.0.1",
        "zendframework/zend-expressive-authentication": "^1.0",
        "zendframework/zend-expressive-authentication-zendauthentication": "^1.0@dev",
        "zendframework/zend-expressive-authorization-acl": "^1.0@dev",
        "zendframework/zend-expressive-helpers": "^5.0",
        "zendframework/zend-expressive-zendrouter": "^3.0",
        "zendframework/zend-expressive-zendviewrenderer": "^2.0",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^2.9.1",
        "zendframework/zend-expressive-tooling": "^1.0",
        "zfcampus/zf-development-mode": "^3.1",
        "filp/whoops": "^2.1.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    }
}
